{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "TaskManagerDatabase": "Server=(localDB)\\MSSQLLocalDB;Database=TaskManager;Integrated Security=True;TrustServerCertificate=true"
  },
  "Kestrel": {
    "TaskManager.API": {
      "Http": {
        "Url": "http://localhost:5262",
        "Port": "5262"
      },
      "Https": {
        "Url": "https://localhost:7001",
        "Port": "7001"
      }
    },
    "TaskManager.Service": {
      "Http": {
        "Url": "http://localhost:5263",
        "Port": "5263"
      },
      "Https": {
        "Url": "https://localhost:7002",
        "Port": "7002"
      }
    }
  },
  "GrpcClient": {
    "Url": "https://localhost:5001"
  },
  "gRPCPort": "5001",
  "Jwt": {
    "SecretKey": "SuperSecretKey123",
    "Issuer": "TaskManagerApplication", // Who create the token
    "Audience": "TaskManagerUser", // Who receive the token
    "ExpireMinutes": 60
  },
  "RabbitMQ": {
    "HostName": "localhost",
    "UserName": "guest",
    "Password": "guest",
    "QueueName": "taskQueue",
    "ExchangeName": "taskExchange"
  }
}
